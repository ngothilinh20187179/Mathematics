   "source": [
    "import sympy as sp\n",
    "\n",
    "x = sp.Symbol('x')\n",
    "f = 2 * x\n",
    "\n",
    "# integral 0 -> 2\n",
    "integral_value = sp.integrate(f, (x, 0, 2))\n",
    "\n",
    "print(\"∫₀² 2x dx =\", integral_value)\n"
   ]
  }
